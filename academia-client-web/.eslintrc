{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "never"],
    "class-methods-use-this": "off",
    "max-len": "off",
    "no-console": "off",

    "react/jsx-filename-extension": "off",
    "react/no-did-mount-set-state": "off",
    "jsx-a11y/anchor-is-valid": "off"
  }
}
